//input YES OR no
int main()
{
	int ch=0;
	while((ch=getchar())!=EOF)
	{
		if(isalpha(ch))
		printf("YES\n");
		else
		printf("no\n");
		getchar();
	}
	return 0;
}
input
  1
1   1
.......
int main()
{
	char ch=0;
	scanf("%c",&ch);
	int i=0;
	for(i=0;i<5;i++);
	{
		int j=0;
		
	for(j=0;j<5-i-1;i++);
	{
		printf(" ");
	}
		for(j=0;j<=i;j++)
		{
			printf("%c",ch);
		}
		printf("\n");
	}
	return 0;
}
